<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="namlit.slackforce" >

    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SelectWebbingManufacturerActivity"
            android:label="@string/title_activity_select_webbing_manufactorer_action" >
        </activity>
        <activity
            android:name=".SelectWebbingActivity"
            android:label="@string/title_activity_select_webbing" >
        </activity>
        <activity
            android:name=".ChooseResultParameterActivity"
            android:label="@string/title_activity_choose_result_parameter" >
        </activity>
    </application>

</manifest>
